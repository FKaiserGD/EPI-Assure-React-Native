{
  "expo": {
    "name": "epi-assure",
    "slug": "epi-assure",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.webp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Precisamos da permissão de acesso à câmera para isso."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.webp",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}